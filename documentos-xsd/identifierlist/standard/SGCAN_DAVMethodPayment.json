{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"description": "INTERCOM - Medio de pago",
	"type": "object",
	"properties": {
		"DAVMethodPayment:DAVMethodPayment": {
			"$ref": "#/definitions/.DAVMethodPayment:DAVMethodPayment"
		}
	},
	"additionalProperties": false,
	"definitions": {
		".DAVMethodPayment:DAVMethodPayment": {
			"$ref": "#/definitions/DAVMethodPayment:DAVMethodPaymentType"
		},
		"DAVMethodPayment:DAVMethodPaymentType": {
			"type": "object",
			"properties": {
				"$extensionProperty": {
					"$ref": "#/definitions/DAVMethodPayment:MethodPaymentCodeList"
				},
				"@schemeAgencyID": {
					"$ref": "#/definitions/string"
				},
				"@schemeAgencyName": {
					"$ref": "#/definitions/string"
				},
				"@schemeDataURI": {
					"$ref": "#/definitions/anyURI"
				},
				"@schemeID": {
					"$ref": "#/definitions/token"
				},
				"@schemeName": {
					"$ref": "#/definitions/string"
				},
				"@schemeURI": {
					"$ref": "#/definitions/anyURI"
				},
				"@schemeVersionID": {
					"$ref": "#/definitions/token"
				}
			},
			"required": [
				"@schemeID"
			],
			"additionalProperties": false
		},
		"DAVMethodPayment:MethodPaymentCodeList": {
			"type": "string",
			"enum": [
				"01",
				"02",
				"03",
				"04",
				"05",
				"06",
				"07"
			]
		},
		"anyURI": {
			"type": "string"
		},
		"string": {
			"type": "string"
		},
		"token": {
			"type": "string",
			"pattern": "^\\S+$"
		}
	}
}