{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"description": "INTERCOM - Forma de pago",
	"type": "object",
	"properties": {
		"DAVFormPayment:DAVFormPayment": {
			"$ref": "#/definitions/.DAVFormPayment:DAVFormPayment"
		}
	},
	"additionalProperties": false,
	"definitions": {
		".DAVFormPayment:DAVFormPayment": {
			"$ref": "#/definitions/DAVFormPayment:DAVFormPaymentType"
		},
		"DAVFormPayment:DAVFormPaymentType": {
			"type": "object",
			"properties": {
				"$extensionProperty": {
					"$ref": "#/definitions/DAVFormPayment:FormPaymentCodeList"
				},
				"@schemeAgencyID": {
					"$ref": "#/definitions/string"
				},
				"@schemeAgencyName": {
					"$ref": "#/definitions/string"
				},
				"@schemeDataURI": {
					"$ref": "#/definitions/anyURI"
				},
				"@schemeID": {
					"$ref": "#/definitions/token"
				},
				"@schemeName": {
					"$ref": "#/definitions/string"
				},
				"@schemeURI": {
					"$ref": "#/definitions/anyURI"
				},
				"@schemeVersionID": {
					"$ref": "#/definitions/token"
				}
			},
			"required": [
				"@schemeID"
			],
			"additionalProperties": false
		},
		"DAVFormPayment:FormPaymentCodeList": {
			"type": "string",
			"enum": [
				"01",
				"02",
				"03",
				"04",
				"05",
				"06"
			]
		},
		"anyURI": {
			"type": "string",
			"format": "uri"
		},
		"string": {
			"type": "string"
		},
		"token": {
			"type": "string",
			"pattern": "^\\S+$"
		}
	}
}